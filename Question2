{
  "finalQuery": "SELECT e1.EMP_ID, e1.FIRST_NAME, e1.LAST_NAME, e1.DEPARTMENT_NAME, COUNT(e2.EMP_ID) AS YOUNGER_EMPLOYEES_COUNT FROM Employee e1 JOIN Employee e2 ON e1.DEPARTMENT_NAME = e2.DEPARTMENT_NAME AND e1.AGE > e2.AGE GROUP BY e1.EMP_ID, e1.FIRST_NAME, e1.LAST_NAME, e1.DEPARTMENT_NAME ORDER BY e1.EMP_ID DESC;"
}
